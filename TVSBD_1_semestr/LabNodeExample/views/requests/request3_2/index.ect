<%extend '././page.ect' %>
<table>
    <tr>
        <th>Логин</th>
        <th>Пароль</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Отчество</th>
        <th>Категория</th>
        <th>Номер прав</th>
        <th>Дата выдачи</th>
    </tr>
    <% for data in @datas : %>
    <tr>
        <td><%= data.login %></td>
        <td><%= data.password %></td>
        <td><%= data.name %></td>
        <td><%= data.surname %></td>
        <td><%= data.patronymic %></td>
        <td><%= data.category %></td>
        <td><%= data.license_num %></td>
        <td><%= data.license_date %></td>
    </tr>
    <% end %>
</table>
<p><a href="/requests">Назад</a></p>